# jar(1) completion                                        -*- shell-script -*-

_jar()
{
    local cur prev words cword comp_args
    _comp_initialize -- "$@" || return

    if ((cword == 1)); then
        COMPREPLY=($(compgen -W 'c t x u' -- "$cur"))
        return
    fi

    case ${words[1]} in
        *c*f)
            compopt -o bashdefault -o default
            ;;
        *f)
            _filedir_xspec unzip "${@:2}"
            ;;
        *)
            compopt -o bashdefault -o default
            ;;
    esac
} &&
    complete -F _jar jar

# ex: filetype=sh
